{
  "name": "@r3e/neo-n3-mcp",
  "version": "1.3.0",
  "description": "MCP server and HTTP API for Neo N3 blockchain integration",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "test": "jest",
    "test:core": "node tests/rpc-compatibility-test.js",
    "test:integration": "node tests/mcp-integration-test.js",
    "test:http": "node tests/mcp-http-test.js",
    "example:core": "node examples/core-functionality.js",
    "postinstall": "node -e \"try { require('fs').mkdirSync('wallets', { recursive: true }); } catch (e) { if (e.code !== 'EEXIST') throw e; }\"",
    "prepublishOnly": "npm run build"
  },
  "bin": {
    "neo-n3-mcp": "./dist/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "API.md",
    "TESTING.md",
    "DEPLOYMENT.md",
    "ARCHITECTURE.md",
    "NETWORKS.md",
    "CHANGELOG.md"
  ],
  "keywords": [
    "neo",
    "blockchain",
    "mcp",
    "n3",
    "modelcontextprotocol",
    "http",
    "api",
    "rest"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-typescript": "^7.27.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^18.15.11",
    "axios": "^1.8.4",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@cityofzion/neon-js": "^5.3.0",
    "@modelcontextprotocol/sdk": "^1.10.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
